{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "mankool's Addons",
  "description": "mankool's Meteor Client addons.",
  "authors": [
    "mankool0"
  ],
  "contact": {
    "sources": "https://github.com/mankool0/metor-addons"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.mankool.addon.Addons"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}", "1.21.7", "1.21.6"],
    "meteor-client": "*"
  }
}
